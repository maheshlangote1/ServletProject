<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Servlet_First</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>demo</servlet-name>
    <servlet-class>com.UserRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>demo</servlet-name>
    <url-pattern>/Servlate/UserRegistration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>com.HelloWord</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/Servlate/HelloWord</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>hellouser</servlet-name>
    <servlet-class>com.HelloUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hellouser</servlet-name>
    <url-pattern>/Servlate/HelloUser</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>boot</servlet-name>
    <servlet-class>com.BootstrapRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>boot</servlet-name>
    <url-pattern>/Servlate/BootstrapRegistration</url-pattern>
  </servlet-mapping>
</web-app>